[user]
        name = Kyle Suarez
        email = ksuarz@gmail.com

[core]
        editor = vim
        excludesfile = /Users/ksuarz/.gitignore_global

[alias]
        # Show git aliases
        alias = "!git config -l | grep alias | cut -c 7-"

        # Display commits in graph format
        graph = log --graph --abbrev-commit

        # Remove a file from staging
        unstage = reset --

[color]
        ui = auto

[url "https://github.com/"]
        insteadOf = "github:"
        insteadOf = "gh:"

[url "https://gist.github.com/"]
        insteadOf = "gist:"

[push]
        # By default, a push only pushes the current branch
        default = current

#" vim: filetype=config
